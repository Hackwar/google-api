{
    "name": "joomla/google",
    "type": "joomla-package",
    "description": "Joomla Google Package",
    "keywords": ["joomla", "framework", "google"],
    "homepage": "https://github.com/joomla-framework/google-api",
    "license": "GPL-2.0+",
    "require": {
        "php": "~5.4|~7.0",
        "joomla/uri": "~1.0",
        "joomla/oauth2": "~2.0@dev",
        "joomla/filesystem": "~1.0"
    },
    "require-dev": {
        "joomla/input": "~1.0",
        "joomla/registry": "~2.0@dev",
        "joomla/test": "~1.0",
        "phpunit/phpunit": "~4.8|>=5.0 <5.4",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Google\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\Google\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
	        "dev-2.0-dev": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
